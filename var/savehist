;; -*- mode: emacs-lisp; coding: utf-8-unix -*-
;; Minibuffer history file, automatically generated by ‘savehist’.

(setq savehist-minibuffer-history-variables '(file-name-history minibuffer-history buffer-name-history command-history extended-command-history))
(setq file-name-history '(#("/home/jdt/.emacs.d/" 0 19 (ivy-index 13)) "~/"))
(setq buffer-name-history '("*scratch*" "*Messages*" "*Warnings*"))
(setq command-history '((switch-to-buffer "*scratch*" nil 'force-same-window) (switch-to-buffer "*Messages*" nil 'force-same-window) (execute-extended-command nil "" nil) (execute-extended-command nil "" nil) (quit-window nil) (execute-extended-command nil "quit-window" "quit") (find-file "~/" t) (fringe-mode 0) (execute-extended-command nil "fringe-mode" "frin") (switch-to-buffer "*Warnings*" nil 'force-same-window) (customize) (execute-extended-command nil "customize" "customize-t")))
(setq extended-command-history '("quit-window" "fringe-mode" "customize"))
(setq search-ring 'nil)
(setq regexp-search-ring 'nil)
